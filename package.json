    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.7.5",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "sanitize-html": "^2.7.0",
    "socket.io": "^4.5.2",
    "socket.io-redis": "^6.0.1",
    "typeorm": "^0.3.6",
    "uuid": "^8.3.2",
