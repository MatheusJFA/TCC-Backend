    "nodemailer": "^6.7.0",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.38",
    "uuid": "^8.3.2",
    "winston": "^3.4.0",
    "yup": "^0.32.11"
  },
