    "Matheus José Ferreira de Aguiar"
  ],
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register --transpile-only src/server.ts",
    "dev": "ts-node-dev -r tsconfig-paths/register --inspect --transpile-only --ignore-watch node_modules src/server.ts",
    "test": "cross-env NODE_ENVIROMENT=TEST jest --noStackTrace --forceExit --runInBand src/tests ",
    "test:clearCache": "jest --clearCache",
    "test:staged": "npm test --findRelatedTests",
    "test:coverage": "npm test --coverage",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli -d ./src/configuration/database.ts",
    "migration:create": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli migration:create ./src/infrastructure/migrations/",
    "migration:generate": "npm run typeorm -- migration:generate ./src/infrastructure/migrations",
    "migration:run": "npm run typeorm -- migration:run",
    "migration:revert": "npm run typeorm -- migration:revert",
    "migration:show": "npm run typeorm -- migration:show",
    "schema:sync": "npm run typeorm -- schema:sync"
  },
  "license": "ISC",
